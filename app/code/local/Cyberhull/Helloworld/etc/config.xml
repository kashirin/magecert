<config>    
    <modules>
        <Cyberhull_Helloworld>
            <version>0.1.0</version>
        </Cyberhull_Helloworld>
    </modules>
    
    <frontend>
        <routers>
            <hi>
                <use>standard</use>
                <args>
                    <module>Cyberhull_Helloworld</module>
                    <frontName>custom</frontName>
                </args>
            </hi>
        </routers>
        <layout>
        	<updates>
        		<hi>
        			<file>ch.xml</file>
        		</hi>	
        	</updates>
        </layout>  
    </frontend>
     <global>
		<blocks>
			<cyberhull_helloworld>
				<class>Cyberhull_Helloworld_Block</class>
			</cyberhull_helloworld>
		</blocks>
	</global>
	
	<default>
		<some>
			<random>
				<one>
					<two>
					some - random - one - two
					</two>
				</one>
			</random>
		</some>
	</default>
    
</config>